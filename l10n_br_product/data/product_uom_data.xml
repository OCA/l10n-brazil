<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="True">

    <record id="product_uom_categ_area" model="product.uom.categ">
        <field name="name">Área</field>
    </record>

    <record id="product_uom_categ_energy" model="product.uom.categ">
        <field name="name">Energia</field>
    </record>

    <record id="product_uom_categ_unit_nconv" model="product.uom.categ">
        <field name="name">Unidade não conversível</field>
