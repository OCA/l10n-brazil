<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="True">

    <record id="product_uom_categ_area" model="product.uom.categ">
        <field name="name">Área</field>
    </record>


    <record id="product_uom_categ_energy" model="product.uom.categ">
        <field name="name">Energia</field>
    </record>

     <record id="product_uom_mega_watt_hr" model="product.uom">
        <field name="name">MWh</field>
        <field ref="product_uom_categ_energy" name="category_id"/>
        <field name="factor" eval="1"/>
     </record>

    <record id="product_uom_par" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_unit"/>
        <field name="name">pares</field>
        <field name="factor_inv" eval="2"/>
        <field name="uom_type">bigger</field>
    </record>

    <record id="product_uom_milheiro" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_unit"/>
        <field name="name">milhei</field>
        <field name="factor_inv" eval="1000"/>
        <field name="rounding" eval="0.001"/>
        <field name="uom_type">bigger</field>
    </record>

    <record id="product_uom_quilate" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_kgm"/>
        <field name="name">K</field>
        <field name="factor" eval="5000"/>
        <field name="uom_type">smaller</field>
    </record>

    <record id="product_uom_jogo" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_unit"/>
        <field name="name">jogo</field>
        <field name="factor" eval="1.0"/>
    </record>

    <record id="product_uom_cm_2" model="product.uom">
        <field name="name">cm²</field>
        <field name="category_id" ref="product_uom_categ_area"/>
        <field name="factor">10000.0</field>
        <field name="uom_type">smaller</field>
    </record>

    <record id="product_uom_m_3" model="product.uom">
        <field name="name">m³</field>
        <field name="category_id" ref="product.product_uom_categ_vol"/>
        <field name="factor">1000.0</field>
        <field name="uom_type">bigger</field>
    </record>


    <record id="product_uom_ml" model="product.uom">
        <field name="name">mL</field>
        <field name="category_id" ref="product.product_uom_categ_vol"/>
        <field name="factor">1000.0</field>
        <field name="uom_type">smaller</field>
    </record>

    <record id="product_uom_m_2" model="product.uom">
        <field name="name">m²</field>
        <field name="category_id" ref="product_uom_categ_area"/>
        <field name="factor">1.0</field>
    </record>

    <record id="product_uom_service" model="product.uom">
        <field name="name">serviço</field>
        <field ref="product.uom_categ_wtime" name="category_id"/>
        <field name="factor" eval="8.0"/>
        <field name="uom_type">smaller</field>
    </record>
    
    <record id="product.product_uom_unit" model="product.uom">
        <field name="name">unid</field>
    </record>

    <record id="product.product_uom_gram" model="product.uom">
        <field name="name">gramas</field>
    </record>

    <record id="product.product_uom_kgm" model="product.uom">
        <field name="name">kg</field>
    </record>

    <record id="product.product_uom_ton" model="product.uom">
        <field name="name">ton</field>
    </record>

    <record id="product.product_uom_litre" model="product.uom">
        <field name="name">litro</field>
    </record>

    <record id="product.product_uom_hour" model="product.uom">
        <field name="name">hora</field>
    </record>

    <record id="product.product_uom_day" model="product.uom">
        <field name="name">dia</field>
    </record>

    <record id="product.product_uom_dozen" model="product.uom">
        <field name="name">dúzia</field>
    </record>

    <record id="product.product_uom_gal" model="product.uom">
        <field name="name">galão</field>
    </record>

