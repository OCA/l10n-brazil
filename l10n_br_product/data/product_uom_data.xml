    <record id="product_uom_milheiro" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_unit"/>
        <field name="name">Milheiro</field>
        <field name="codigo">milhei</field>
        <field name="factor_inv" eval="1000"/>
        <field name="rounding" eval="0.001"/>
        <field name="uom_type">bigger</field>
    </record>

     <record id="product_uom_mega_watt_hr" model="product.uom">
        <field name="name">Megawatts por Hora</field>
        <field name="codigo">MWh</field>
        <field ref="product_uom_categ_energy" name="category_id"/>
        <field name="factor" eval="1"/>
     </record>

    <record id="product_uom_par" model="product.uom">
        <field name="category_id" ref="product.product_uom_categ_unit"/>
