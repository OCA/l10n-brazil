<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="l10n_br_account_product_fiscal_category_form" model="ir.ui.view">
            <field name="name">l10n_br_account_product.fiscal.category (in l10n_br_contabilidade)</field>
            <field name="model">l10n_br_account.fiscal.category</field>
            <field name="inherit_id" ref="l10n_br_account.view_l10n_br_account_fiscal_category_form"/>
            <field name="arch" type="xml">
                <xpath expr='//group[@string="Diário Contábil"]' position="after">
                    <separator string="Roteiro Contábil"/>
                    <group colspan="4">
                        <field name="account_event_template_id" />
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
