<odoo>

    <record id="fiscal_res_company_form" model="ir.ui.view">
        <field name="name">l10n_br_fiscal.res.company.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <field name="tax_framework" position="after">
                <field name="cultural_sponsor"/>
                <field name="nfse_environment"/>
                <field name="provedor_nfse"/>
                <field name="nfse_website"/>
                <field name="nfse_city_logo" widget="image"/>
            </field>
        </field>
    </record>

</odoo>
