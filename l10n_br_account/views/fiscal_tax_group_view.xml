<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="fiscal_tax_group_form" model="ir.ui.view">
        <field name="name">fiscal.tax.group.form</field>
        <field name="model">l10n_br_fiscal.tax.group</field>
        <field name="inherit_id" ref="l10n_br_fiscal.tax_group_form"/>
        <field name="arch" type="xml">
            <page name="csts" position="after">
                <page name="account" string="Account">
                    <group name="account_main">
                        <field name="colect_account_id"/>
                        <field name="recover_account_id"/>
                    </group>
                </page>
            </page>
        </field>
    </record>

</odoo>
