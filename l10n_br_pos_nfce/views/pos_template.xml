<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <template
        id="pos_l10n_br_assets_backend"
        name="pos_l10n_br_assets_backend"
        inherit_id="point_of_sale.assets"
    >
            <xpath expr="." position="inside">
                <script
                src="/l10n_br_pos_nfce/static/src/js/models.js"
                type="text/javascript"
            />
                <script
                src="/l10n_br_pos_nfce/static/src/js/Screens/PaymentScreen.js"
                type="text/javascript"
            />
            <script
                src="/l10n_br_pos_nfce/static/src/js/Screens/OrderManagementScreen/ControlButtons/CancelOrderButton.js"
                type="text/javascript"
            />
            <script
                src="/l10n_br_pos_nfce/static/src/js/Screens/ReceiptScreen/NfceOrderReceipt.js"
                type="text/javascript"
            />
            </xpath>

            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link
                rel="stylesheet"
                href="/l10n_br_pos_nfce/static/src/css/ReceiptOrder.css"
            />
            </xpath>
        </template>
</odoo>
