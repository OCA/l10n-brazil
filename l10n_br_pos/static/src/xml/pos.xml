                            <input class='busca-cpf-cnpj' name="busca-cpf-cnpj" placeholder="Insira o CPF/CNPJ"></input>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <button class="input-button number-char btn-busca-cpf-cnpj">Buscar</button>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="row">
                    <div class="col client_desde" style="display:none">
                            <span class='label'>Cliente desde:</span>
                            <span class='date_cliente' name="date_cliente"/>
                    </div>
                </div>
            </div>
        </t>
    </t>
    <t t-extend="ClientLine">
