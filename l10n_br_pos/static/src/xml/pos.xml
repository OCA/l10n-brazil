                    <option value=''>None</option>
                    <t t-foreach='widget.pos.cities' t-as='city'>
                        <option t-att-value='city.id' t-att-selected="partner.l10n_br_city_id ? ((city.id === partner.l10n_br_city_id[0]) ? true : undefined) : undefined">
