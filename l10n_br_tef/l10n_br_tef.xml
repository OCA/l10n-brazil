<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<template id="assets_backend" name="point_of_sale assets" inherit_id="web.assets_backend">
    <xpath expr="." position="inside">
        <script type="text/javascript" src="/l10n_br_tef/static/src/js/l10n_br_tef.js"></script>
    </xpath>
</template>

<template id="index" name="pos_payment_terminal index" inherit_id="point_of_sale.index">
    <xpath expr="//link[@id='pos-stylesheet']" position="after">
        <link rel="stylesheet" href="/l10n_br_tef/static/src/css/l10n_br_tef.css" id="pos_payment_terminal-stylesheet"/>
    </xpath>
</template>

</data>
</openerp>
