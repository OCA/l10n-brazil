<?xml version="1.0"?>

<odoo>
        <record id="tef_credit_card" model="account.journal">
            <field name="name">Credit Card</field>
            <field name="code">CC01</field>
            <field name="type">bank</field>
            <field name="journal_user" eval="True" />
        </record>

        <record id="tef_debit_card" model="account.journal">
            <field name="name">Debit Card</field>
            <field name="code">CD01</field>
            <field name="type">bank</field>
            <field name="journal_user" eval="True" />
        </record>

        <record model="pos.config" id="point_of_sale.pos_config_main">
            <field name="iface_tef" eval="True"/>
            <field name="journal_ids" eval="[(6, 0, [ref('tef_credit_card'), ref('tef_debit_card')])]" />
        </record>

</odoo>

