<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        <record id="action_report_sped_documento_mdfe" model="ir.actions.report.xml">
            <field name="name">MDF-E</field>
            <field name="model">sped.documento</field>
            <field name="report_name">report_sped_documento_mdfe</field>
            <field name="report_type">py3o</field>
            <field name="py3o_filetype">pdf</field>
            <field name="module">l10n_br_mdfe</field>
            <field name="py3o_template_fallback">report/mdfe.odt</field>
            <field name="print_report_name">('MDFe'+(object.chave or '')+'.pdf')	</field>
        </record>

        <record id="button_mdfe" model="ir.values">
            <field name="key2">client_print_multi</field>
            <field name="model">sped.documento</field>
            <field name="name">MDF-E</field>
            <field name="value" eval="'ir.actions.report.xml,%d'%action_report_sped_documento_mdfe" />
        </record>
</odoo>