<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	   
	   	<!-- CRM Lead Form View  -->
	   	<record model="ir.ui.view" id="l10n_br_crm_case_form_view_leads1">
			<field name="name">l10n_br_crm.leads1</field>
			<field name="model">crm.lead</field>
			<field name="inherit_id" ref="crm.crm_case_form_view_leads" />
			<field name="arch" type="xml">
				<field name="city" position="replace">
					<field name="city" invisible="1" />
				</field>
				<field name="state_id" position="replace">
					<field name="state_id" />
					<field name="l10n_br_city_id" placeholder="municipio" on_change="onchange_l10n_br_city_id(l10n_br_city_id)" />
				</field>
				<field name="street2" position="replace">
					<field name="number" placeholder="numero"/>
					<field name="street2" placeholder="complemento"/>
				</field>
				<!-- FIXME field name="zip" position="replace">
					<field name="zip" on_change="onchange_mask_zip(zip)" />
					<button name="zip_search" string="Pesquisar CEP" colspan="2" type="object" icon="gtk-select-all"/>
				</field -->
			</field>
		</record>
	
	</data>
</openerp>
