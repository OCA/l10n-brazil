"                <tr>\n"
"                    <th colspan=\"2\">Detalhamento do Ressarcimento</th>\n"
"                </tr>\n"
"                <tr>\n"
"                    <th>Descrição</th>\n"
"                    <th>Valor</th>\n"
"                </tr>\n"
