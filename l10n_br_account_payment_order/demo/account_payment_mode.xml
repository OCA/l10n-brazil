            eval="[(6,0,[ref('santander_240_return_06'), ref('santander_240_return_09'), ref('santander_240_return_17'), ref('santander_240_return_a4')])]"
        />
        <field name="cnab_sending_code_id" ref="santander_240_instruction_01" />
        <field name="cnab_write_off_code_id" ref="santander_240_instruction_02" />
        <field
            name="cnab_code_change_title_value_id"
            ref="santander_240_instruction_47"
        />
        <field
            name="cnab_code_change_maturity_date_id"
            ref="santander_240_instruction_06"
        />
        <field name="cnab_code_protest_title_id" ref="santander_240_instruction_09" />
        <field
