        />
        <field name="cnab_code_grant_rebate_id" ref="santander_240_instruction_04" />
        <field name="cnab_code_cancel_rebate_id" ref="santander_240_instruction_05" />
        <field name="cnab_code_grant_discount_id" ref="santander_240_instruction_10" />
