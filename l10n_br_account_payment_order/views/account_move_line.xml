<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="cnab_view_move_line_form_inherit" model="ir.ui.view">
        <field name="name">cnab.account.move.line.form.inherit</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_form"/>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            <field name="blocked" position="after">
                <field name="cnab_state"/>
                <field name="payment_situation"/>
                <field name="own_number"/>
                <field name="document_number"/>
                <field name="company_title_identification"/>
                <field name="instructions"/>
            </field>
        </field>
    </record>

</odoo>
