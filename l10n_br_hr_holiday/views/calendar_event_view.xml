<?xml version="1.0" encoding="UTF-8" ?>
<odoo>


        <record id="l10n_br_hr_holiday_calendar_event_view" model="ir.ui.view">
            <field name="name">L10n Br Hr Holiday Calendar Event</field>
            <field name="model">calendar.event</field>
            <field name="inherit_id" ref="calendar.view_calendar_event_form" />
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page[@name='page_misc']" position="inside">
                    <field name="models_id" invisible="1"/>
                </xpath>
            </field>
        </record>


</odoo>
