<odoo>

    <record model="ir.ui.view" id="l10n_br_fiscal_natureza_operacao_form_view">
        <field name="name">l10n_br_fiscal.natureza.operacao.form (in
            l10n_br_fiscal)
        </field>
        <field name="model">l10n_br_fiscal.natureza.operacao</field>
        <field name="arch" type="xml">
            <form>
                <header>
                </header>
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                    </group>
                </sheet>
                <div class="oe_chatter"></div>
            </form>
        </field>
    </record>

    <record model="ir.ui.view"
            id="l10n_br_fiscal_natureza_operacao_search_view">
        <field name="name">l10n_br_fiscal.natureza.operacao.search (in
            l10n_br_fiscal)
        </field>
        <field name="model">l10n_br_fiscal.natureza.operacao</field>
        <field name="arch" type="xml">
            <search>
                <field name="code"/>
                <field name="name"/>
            </search>
        </field>
    </record>

    <record model="ir.ui.view" id="l10n_br_fiscal_natureza_operacao_tree_view">
        <field name="name">l10n_br_fiscal.natureza.operacao.tree (in l10n_br_fiscal)
        </field>
        <field name="model">l10n_br_fiscal.natureza.operacao</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>
