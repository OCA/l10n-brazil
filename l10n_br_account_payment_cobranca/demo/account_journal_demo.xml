<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- Diarios -->

    <!-- Diario Bradesco -->
    <record id="bradesco_journal" model="account.journal">
        <field name="name">Banco Bradesco</field>
        <field name="type">bank</field>
        <field name="code">BNC2</field>
        <field name="inbound_payment_method_ids"
               eval="[(6,0,[ref('l10n_br_account_payment_cobranca.payment_mode_type_cnab240'),
               ref('l10n_br_account_payment_cobranca.payment_mode_type_cnab400')])]"/>
        <field name="bank_account_id" ref="l10n_br_account_payment_order.main_company_bank_bradesco"/>
    </record>

